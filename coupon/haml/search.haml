/[if IE 8 ]  <html lang="en" class="no-js ie8">
/[if IE 9 ]  <html lang="en" class="no-js ie9">


%head
  %meta{:charset => "utf-8"}

  %title
  %meta{:name => "description", :content => ""}
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  -# %meta{:name => "keywords", :content => ""}
  -# %meta{:name => "author", :content => ""}

  %meta{:name => "viewport", :content => "width=device-width, initial-scale=1"}

  %link{:rel => "stylesheet", :href => "../css/sprite.css"}
  %link{:rel => "stylesheet", :href => "../css/style.css"}

  %script{:src => "https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js" }
  -#%script{:src => "https://oss.maxcdn.com/respond/1.4.2/respond.min.js" }

  %body
    .bg-wrapper
      .bg-wrapper_bottom
        .topline
        %header.header

        %nav.main-menu_wrapper.collapse
          .container
            %ul.main-menu.clear-list
              %li.main-menu_item
                %a.main-menu_link{ :href => "index.html" } Home
              %li.main-menu_item
                %a.main-menu_link{ :href => "categories.html" } Categories
              %li.main-menu_item
                %a.main-menu_link{ :href => "company.html" } Events
              %li.main-menu_item
                %a.main-menu_link{ :href => "deal-alerts.html" } Deal alerts
              %li.main-menu_item
                %a.main-menu_link{ :href => "blog.html" } Blog
              %li.main-menu_item
                %a.main-menu_link{ :href => "#" } Get the app
            .clearfix.full-width_wrapper.visible-xs-block
              %button.orange-btn.full-width.-medium SUBMIT COUPON
        %form.search-xs.collapse.hidden-sm.hidden-md.hidden-lg
          %input.standard-input{ :type => "text", :placeholder => "Search" }
        .container
          .row.main-top-section
            .left-column_top
              %h3.search-results_subtitle Search Results for “Pizza Coupons”
              %h1.search-results_title Related Pages
              .search-pages_wrapper
                .row.deal_item
                  .deal_item-col-1
                    .white-wrapper.ie-border
                      %img.img-responsive{:src => "../files/Pizza_Hut_logo.png", :alt => ""}/
                  .deal_item-col-2-3.-search
                    .row
                      .deal_item-col-2
                        .search-pages_descr Pizza Hut Coupons (25)
                      .deal_item-col-3
                        %a.simple-btn.link-btn.-wide  Visit Page
              .gray-shadow
              .deal-wrapper
                %h2.h1.search-related-title Related Coupons
                .row.deal_item
                  .deal_item-col-1
                    .white-wrapper.ie-border
                      %img.img-responsive{:src => "../files/hotdeal2.jpg", :alt => ""}/
                  .deal_item-col-2-3
                    .row
                      .deal_item-col-2
                        .deal-descr
                          Macy's 10% Off Domestic
                          %br
                          Visitor

                      .deal_item-col-3
                        %a.getdeal.link-btn GET DEAL
                    .row
                      .col-xs-18
                        %a.simple-btn.link-btn See more Ralf Lauren
                .gray-shadow
                .row.deal_item
                  .deal_item-col-1
                    .white-wrapper.ie-border
                      %img.img-responsive{:src => "../files/hotdeal2.jpg", :alt => ""}/
                  .deal_item-col-2-3
                    .row
                      .deal_item-col-2
                        .deal-descr
                          Ralph Lauren Printable
                          %br
                          Coupon - $20-$150 Off

                      .deal_item-col-3
                        %a.showcoupon.link-btn  FIVE5OFF
                    .row
                      .col-xs-18
                        %a.simple-btn.link-btn See more Ralf Lauren coupons
                .gray-shadow
                .row.deal_item
                  .deal_item-col-1
                    .white-wrapper.ie-border
                      %img.img-responsive{:src => "../files/hotdeal2.jpg", :alt => ""}/
                  .deal_item-col-2-3
                    .row
                      .deal_item-col-2
                        .deal-descr
                          50 % of any sushi at
                          %br
                          TBZY

                      .deal_item-col-3
                        %a.getdeal.link-btn GET DEAL
                    .row
                      .col-xs-18
                        %a.simple-btn.link-btn See more TBZY coupons
                .gray-shadow

                .row.deal_item
                  .deal_item-col-1
                    .white-wrapper.ie-border
                      %img.img-responsive{:src => "../files/hotdeal2.jpg", :alt => ""}/
                  .deal_item-col-2-3
                    .row
                      .deal_item-col-2
                        .deal-descr
                          Bath and Body Works
                          %br
                          Coupon

                      .deal_item-col-3
                        %a.getdeal.link-btn GET DEAL
                    .row
                      .col-xs-18
                        %a.simple-btn.link-btn See more Bath and Body coupons
                .gray-shadow

            .right-column_top
              %section.right-section
                .sign-up_wrapper.white-wrapper
                  .sign-up
                    %h3.sign-up_title Get the latest deal sent to you inbox.
                    %h4.sign-up_list_title BENEFITS:
                    %ul.sign-up_list.clear-list
                      %li.sign-up_list_item.media
                        %i.checkmark.icon
                        %span.media-body  Choose coupons you are interested in
                      %li.sign-up_list_item.media
                        %i.checkmark.icon
                        %span.media-body Be the first to know on new deals
                      %li.sign-up_list_item.media
                        %i.checkmark.icon
                        %span.media-body Exclusive offers to e-mail subscribers
                      %li.sign-up_list_item.media
                        %i.checkmark.icon
                        %span.media-body And much more!
                .sign-up_form
                  .sign-up_form_inner
                    .sign-up_input-wrapper
                      %input.standard-input.-medium.-green-text.-green-bg.-icon-left{ :type => "text", :placeholder => "Your e-mail" }
                      %i.mail.icon
                      %i.gray-line.icon
                    %button.orange-btn.-medium.-green-bg
                      %i.share.icon
                      %span SIGN ME UP!
              %section.right-section.banner-right_wrapper
                .banner-right
                  %img.img-responsive{:src => "../files/banner.jpg", :alt => ""}/
                  .banner-right_title Pizza
                  %img.banner-right_logo{:src => "../files/banner-logo.png", :alt => ""}/
                  .banner-right_status DAILY BEST



        %footer
          .container
            .row
              .footer-col-1.footer-col
                .footer-col_inner
                  %h3.footer_title.hidden-xs.hidden-sm C
                  %ul.company-list.clear-list
                    %li.company-list_item
                      %a.company-list_link{ :href => "#" }
                        %i.icon.markdown-circle.hidden-sm.hidden-xs
                        %span A
                    %li.company-list_item
                      %a.company-list_link{ :href => "#" }
                        %i.icon.markdown-circle.hidden-sm.hidden-xs
                        %i.icon.markdown-small.visible-sm-inline-block.visible-xs-inline-block
                        %span A
                    %li.company-list_item
                      %a.company-list_link{ :href => "#" }
                        %i.icon.markdown-circle.hidden-sm.hidden-xs
                        %i.icon.markdown-small.visible-sm-inline-block.visible-xs-inline-block
                        %span C
              .footer-col-2.footer-col
                .footer-col_inner

              .footer-col-3.ta-c.get-the-app.footer-col
                .footer-col_inner

              .footer-col-4.footer-col


          .footer-bottom
            .container
              .row
                .pull-right.footer-bottom_link-wrapper.col-sm-9.col-xs-18.footer-bottom-col-1
                  %a.footer-bottom_link{ :href => "#" }

                .pull-left.copyright.col-sm-9.col-xs-18
                  %span


