        //  button.switch-btn.-yellow.active.js-switch-btn Опубликовать
        //  button.switch-btn.-yellow.js-switch-btn Не публиковать
        .row.results-tour-item-row
          .col-md-5.col-sm-6
            a(href="landing.html")
              img.img-responsive.results-tour-img(src="#{tour.img}")
          .col-md-7.col-sm-6
            h2.block-title #{tour.title}
            p.md  #{tour.descr}
            .row.results-tour-info
              .col-md-6.col-sm-6.hidden-sm.hidden-xs
                .media
                  .round-img.-md.fll.-left
                    img.img-responsive.-blue-border(src="#{tour.imgguide}")
                  .guide-wrapper.media-body
                     h4.-name #{tour.guidename}
                     h5.-subname #{tour.guidesubtitle}
              .col-md-6.col-sm-6.col-xs-6
                .results-tour-type.icon-text
                  .almost-sqr-wrapper.-md.icon-text_icon
                    i(class="icon #{tour.typeicon}")
                  .icon-text_text
                    span.icon-text_text-inner #{tour.type}, #{tour.subtype}
              .col-md-6.col-sm-6.col-xs-6
                .results-tour-type.icon-text
                  .almost-sqr-wrapper.-md.icon-text_icon
                    i.icon.geo
                  .icon-text_text
                    span.icon-text_text-inner #{tour.place}

            .results-tour-footer.hidden-sm.hidden-xs
              .row
                .icon-text.col-md-4.col-lg-5
                  i.calendar-lg.count-day-wrapper.icon-text_icon
                    span.count-day #{tour.duration}
                    span.days-text  дней
                  .icon-text_text.-md
                    span.icon-text_text-inner.-md #{tour.data}
